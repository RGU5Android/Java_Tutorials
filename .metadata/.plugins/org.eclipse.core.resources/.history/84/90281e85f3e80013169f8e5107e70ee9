		System.out.println("Enter the Value for 2D String Array of Size 3x3");
		String [][]mStringArray = new String[3][];
		for (int row = 0; row < mStringArray.length; row++) {
			for (int col = 0; col < mStringArray[row].length; col++) {
				System.out.println("Enter String Value for " + row + "x" + col);
				mStringArray[row][col] = mScanner.next();
			}
		}
		if (mScanner != null) {
			mScanner.close();
		}

	}

